{
	"manifest_version": 3,
	"name": "__MSG_extensionName__",
	"short_name": "Bonjourr",
	"version": "21.2.1",
	"description": "__MSG_extensionDesc__",
	"author": "Victor Azevedo and Tahoe Beetschen",
	"default_locale": "en",
	"icons": {
		"128": "src/assets/favicons/favicon-128x128.png"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"action": {
		"title": "__MSG_extensionName__"
	},
	"offline_enabled": true,
	"background": {
		"service_worker": "src/scripts/service-worker.js"
	},
	"permissions": [
		"storage"
	],
	"optional_permissions": [
		"bookmarks",
		"topSites",
		"search"
	]
}
