{
	"manifest_version": 3,
	"name": "__MSG_extensionName__",
	"short_name": "Bonjourr",
	"version": "21.1.0",
	"description": "__MSG_extensionDesc__",
	"author": "Victor Azevedo and Tahoe Beetschen",
	"default_locale": "en",
	"icons": {
		"128": "src/assets/favicons/favicon-128x128.png"
	},
	"chrome_settings_overrides": {
		"homepage": "index.html"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"action": {
		"default_icon": {
			"128": "src/assets/favicons/favicon-128x128.png"
		},
		"default_title": "__MSG_extensionName__"
	},
	"background": {
		"service_worker": "src/scripts/service-worker.js"
	},
	"permissions": [
		"storage"
	]
}
